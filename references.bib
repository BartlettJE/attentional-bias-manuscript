@article{kang_individual_2012,
	title = {Individual differences in smoking-related cue reactivity in smokers: {An} eye-tracking and {fMRI} study},
	volume = {38},
	issn = {02785846},
	shorttitle = {Individual differences in smoking-related cue reactivity in smokers},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0278584612000851},
	doi = {10.1016/j.pnpbp.2012.04.013},
	abstract = {Measures of cue reactivity provide a means of studying and understanding addictive behavior. We wanted to examine the relationship between different cue reactivity measures, such as attentional bias and subjective craving, and functional brain responses toward smoking-related cues in smokers. We used eye-tracking measurements, a questionnaire for smoking urges-brief and functional magnetic resonance imaging to assess the responses to smoking-related and neutral visual cues from 25 male smokers after 36 h of smoking abstinence. Regression analyses were conducted to determine the correlation between cue-evoked brain responses and the attentional bias to smoking-related cues. The eye gaze dwell time percentage was longer in response to smoking-related cues than neutral cues, indicating signiﬁcant differences in attentional bias towards smoking-related cues. The attentional bias to smoking-related cues correlated with subjective craving ratings (r = 0.660, p b 0.001). The dorsolateral prefrontal cortex, the putamen, the posterior cingulate cortex and the primary motor cortex were associated with the attentional bias to smoking-related cues, whereas the orbitofrontal cortex, the insula and the superior temporal gyrus were associated with smoking-related cue-induced craving and smoking urges. These results suggest that attentional mechanisms in combination with motivational and reward-related mechanisms play a role in smoking-related cue reactivity. We conﬁrmed a positive correlation between different smoking-related cue reactivities, such as attentional bias and subjective craving, and functional brain responses in various individuals. Further studies in this ﬁeld might contribute to a better individualized understanding of addictive behavior.},
	language = {en},
	number = {2},
	urldate = {2018-11-18},
	journal = {Progress in Neuro-Psychopharmacology and Biological Psychiatry},
	author = {Kang, O-Seok and Chang, Dong-Seon and Jahng, Geon-Ho and Kim, Song-Yi and Kim, Hackjin and Kim, Jong-Woo and Chung, Sun-Yong and Yang, Seung-In and Park, Hi-Joon and Lee, Hyejung and Chae, Younbyoung},
	month = aug,
	year = {2012},
	pages = {285--293},
	file = {Kang et al. - 2012 - Individual differences in smoking-related cue reac.pdf:/Users/JamesBartlett/Documents/PhD/Research/Attentional Bias/Kang et al. - 2012 - Individual differences in smoking-related cue reac.pdf:application/pdf},
}

