	keywords = {Nicotine, Light smokers, Black smokers, Carcinogen exposure, Correlates of exposure},
	pages = {509--520},
	file = {ScienceDirect Snapshot:/Users/JamesBartlett/Zotero/storage/4JEQTECP/S0027968419300173.html:text/html},
}

