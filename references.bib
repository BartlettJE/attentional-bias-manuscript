@article{christiansen_less_2015,
	series = {'{Contemporary} {Perspectives} on {Cognition} in {Addictive} {Behaviors}'},
	title = {Less than meets the eye: {Reappraising} the clinical relevance of attentional bias in addiction},
	volume = {44},
	issn = {0306-4603},
	shorttitle = {Less than meets the eye},
	url = {http://www.sciencedirect.com/science/article/pii/S0306460314003372},
	doi = {10.1016/j.addbeh.2014.10.005},
	abstract = {Recent years have seen an explosion of interest in attentional bias in addiction, particularly its clinical relevance. Specifically, numerous articles claimed to demonstrate either that (1) attentional bias measured in treatment settings could predict subsequent relapse to substance use, or (2) direct modification of attentional bias reduced substance use and improved treatment outcomes. In this paper, we critically evaluate empirical studies that investigated these issues. We show that the evidence regarding both of these claims is decidedly mixed, and that many of the studies that appear to yield positive findings have serious methodological and statistical limitations. We contend that the available literature suggests that attentional bias for drug cues fluctuates within individuals because it is an output of the underlying motivational state at that moment in time, but there is no convincing evidence that it exerts a causal influence on substance use. Future research should make use of experience sampling methodology to characterise the clinical significance of fluctuations in attentional bias over time.},
	language = {en},
	urldate = {2020-01-12},
	journal = {Addictive Behaviors},
	author = {Christiansen, Paul and Schoenmakers, Tim M. and Field, Matt},
	month = may,
	year = {2015},
	keywords = {Attentional bias, Visual probe task, Stroop, Attentional bias modification},
	pages = {43--50},
	file = {ScienceDirect Full Text PDF:/Users/jamesbartlett/Zotero/storage/PMJGDLAD/Christiansen et al. - 2015 - Less than meets the eye Reappraising the clinical.pdf:application/pdf;ScienceDirect Snapshot:/Users/jamesbartlett/Zotero/storage/S5R84TUD/S0306460314003372.html:text/html},
}

