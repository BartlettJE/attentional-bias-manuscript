	urldate = {2019-05-16},
	journal = {Journal of the National Medical Association},
	author = {St.Helen, Gideon and Benowitz, Neal L. and Ahluwalia, Jasjit S. and Tyndale, Rachel F. and Addo, Newton and Gregorich, Steven E. and Pérez-Stable, Eliseo J. and Cox, Lisa Sanderson},
	month = may,
