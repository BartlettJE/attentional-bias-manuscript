@article{anwyl-irvine_gorilla_2019,
	title = {Gorilla in our {Midst}: {An} online behavioral experiment builder},
	volume = {52},
	copyright = {© 2019, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NonCommercial-NoDerivs 4.0 International), CC BY-NC-ND 4.0, as described at http://creativecommons.org/licenses/by-nc-nd/4.0/},
	shorttitle = {Gorilla in our {Midst}},
	url = {https://www.biorxiv.org/content/10.1101/438242v4},
	doi = {10.1101/438242},
	abstract = {{\textless}p{\textgreater}Behavioural researchers are increasingly conducting their studies online to gain access to large and diverse samples that would be difficult to get in a laboratory environment. However, there are technical access barriers to building experiments online, and web-browsers can present problems for consistent timing -- an important issue with reaction time-sensitive measures. For example, to ensure accuracy and test-retest reliability in presentation and response recording, experimenters need a working knowledge of programming languages such as JavaScript. We review some of the previous and current tools for online behavioural research, and how well they address the issues of usability and timing. We then present The Gorilla Experiment Builder (gorilla.sc) a fully tooled experiment authoring and deployment platform, designed to resolve many timing issues, and make reliable online experimentation open and accessible to a wider range of technical abilities. In order to demonstrate the platform9s aptitude for accessible, reliable and scalable research, we administered the task with a range of participant groups (primary school children and adults), settings (without supervision, at home, and under supervision, in schools and public engagement events), equipment (own computers, computer supplied by researcher), and connection types (personal internet connection, mobile phone 3G/4G). We used a simplified flanker task, taken from the Attentional Networks task (Rueda et al., 2004). We replicated the 9conflict network9 effect in all these populations, demonstrating the platform9s capability to run reaction time-sensitive experiments. Unresolved limitations of running experiments online are then discussed, along with potential solutions, and some future features of the platform.{\textless}/p{\textgreater}},
	language = {en},
	urldate = {2019-03-05},
	journal = {Behavior Research Methods},
	author = {Anwyl-Irvine, Alexander and Massonnié, Jessica and Flitton, Adam and Kirkham, Natasha and Evershed, Jo},
	month = feb,
	year = {2019},
	pages = {388--407},
	annote = {
Miller et al. (2018) - new browsers have comparable RTs to the lab, even in domestic environments. 
How Gorilla fixes timing issues

If there are any problems with connection speed, Gorilla loads several trials in advance from the start until the end of the task.
Gorilla times each frame's actual duration rather than assuming it will be 16.667ms. 


Study one successfully replicated the flanker effect with effects of less than 100ms. This requires accurate stimulus presentation and response collection. 
There was no significant difference between groups, despite taking place in children across different class rooms, and the amount of distractions around. 
Study two did not permit access on a tablet or phone. 
The effect was 29.1ms and online studies may produce smaller differences due to the variance in testing environment. Online participants could also be more attentive leading to smaller within-subject variance.
The timing is good in Gorilla, but limited by the sampling rate of the hardware being used. On individual trials, it will not be possible to have accuracy below 8ms. 

 

Gorilla will provide a breakdown of the Browser, operating system, and the viewport characteristics of the web browser (horizontal and vertical pixels). 
},
	file = {Full Text PDF:/Users/JamesBartlett/Zotero/storage/E64EHUCM/Anwyl-Irvine et al. - 2019 - Gorilla in our Midst An online behavioral experim.pdf:application/pdf;Snapshot:/Users/JamesBartlett/Zotero/storage/W482WYUD/438242v4.html:text/html},
}

@article{brand_assessing_2012,
	title = {Assessing the {Effects} of {Technical} {Variance} on the {Statistical} {Outcomes} of {Web} {Experiments} {Measuring} {Response} {Times}},
	volume = {30},
	issn = {0894-4393, 1552-8286},
	url = {http://journals.sagepub.com/doi/10.1177/0894439311415604},
	doi = {10.1177/0894439311415604},
	abstract = {A simulation was conducted to assess the effect of technical variance on the statistical power of web experiments measuring response times. The results of the simulation showed that technical variance reduced the statistical power and the accuracy of the effect size estimate by a negligible magnitude. This finding therefore suggests that researchers’ preconceptions concerning the unsuitability of web experiments for conducting research using response time as a dependent measure are misguided.},
	language = {en},
	number = {3},
	urldate = {2019-03-06},
	journal = {Social Science Computer Review},
	author = {Brand, Andrew and Bradley, Michael T.},
	month = aug,
	year = {2012},
	pages = {350--357},
	annote = {Introduction 

RQ: Simulation to determine the degree that power and accuarcy of the effect is reduced by higher technical variance in web based studies. 

Method

Simulation using R. See for details. 

Results

The maximal power loss was a 3\% decrease due to technical variance. 
Under conditions that maximise technical variance, the loss of statistical power and accuracy of the effect size estimate is minimal across small, medium and large effect sizes. 
