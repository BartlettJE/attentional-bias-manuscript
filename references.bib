	pages = {693--701},
	file = {PubMed Central Full Text PDF:/Users/jamesbartlett/Zotero/storage/DE4MCHC8/Ramo et al. - 2011 - Reliability and validity of self-reported smoking .pdf:application/pdf},
}

@article{price_empirical_2015,
